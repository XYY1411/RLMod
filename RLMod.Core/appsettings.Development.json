{
    "Logging": {
        "LogLevel": {
            "Default": "Debug",
            "System": "Information",
            "Microsoft": "Information"
        }
    },
    "NLog": {
        "rules": {
            "1" : {
                "logger": "*",
                "minLevel": "Off",
                "writeTo": "logfile"
            }
        }
    }
}